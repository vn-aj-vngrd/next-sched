generator client {
    provider        = "prisma-client-js"
    previewFeatures = ["referentialIntegrity"]
}

datasource db {
    provider             = "cockroachdb"
    url                  = env("DATABASE_URL")
    referentialIntegrity = "prisma"
}

model Schedule {
    id      String @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
    classes String
}
